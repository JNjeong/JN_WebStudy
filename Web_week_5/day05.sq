/* 날짜 관련 함수 */
--TO_CHAR
SELECT TO_CHAR(SYSDATE, 'YY/MM/DD/HH24:MI') AS "오늘날짜", SYSDATE+1 AS 하루더한날짜, SYSDATE-1 어제날짜 FROM DUAL;

--TO_DATE
SELECT TO_CHAR(SYSDATE, 'YY/MM/DD/HH24:MI') AS "오늘날짜", TO_DATE('2021-09-30') - TO_DATE('2021-09-25') AS "09/25~09/30" FROM DUAL;
SELECT TO_CHAR(SYSDATE, 'YY/MM/DD/HH24:MI') AS "오늘날짜", TO_DATE('20210930') - TO_DATE('20210925') AS "09/25~09/30" FROM DUAL;

SELECT TO_CHAR(SYSDATE, 'YY/MM/DD/HH24:MI') AS "오늘날짜", MONTHS_BETWEEN(TO_DATE('2021-09-25'), TO_DATE('2021-09-30')) AS "09/25~09/30" FROM DUAL;